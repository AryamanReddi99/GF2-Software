Devices:{
    D, F62 are NAND gates
    (D1 => D12 are NAND gates)
    A, B have 2 inputs
}

Inputs: {
    S1, S2 are switches
    (S1 => S12 are switches)
    (S1 is 1)
}

Connections: {
    Device A: {
        S1 to A.1
        B to A.2
     }
    Device B: {
        A to B.1
        S2 to B.2
     }
}

Monitor:{
    A,B
}
